    "class power:\n",
    "    def __init__(self,base,exponent):\n",
    "        self.base=base\n",
    "        self.exponent=exponent\n",
    "    def get_power(self):\n",
    "        return self.base**self.exponent\n",
    "p=power(10,2)\n",
    "p.get_power()"
