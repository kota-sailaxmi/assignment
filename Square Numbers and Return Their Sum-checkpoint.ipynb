    "class Point:\n",
    "    def __init__(self, x, y, z):\n",
    "        self.x = x\n",
    "        self.y = y\n",
    "        self.z = z\n",
    "\n",
    "    def sq_sum(self):\n",
    "        print(self.x ** 2 + self.y ** 2 + self.z ** 2)\n",
    "\n",
    "s1 = Point(2, 3, 4)\n",
    "s1.sq_sum()"
   ]
